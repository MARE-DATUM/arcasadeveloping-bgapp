            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Angola Marine Biodiversity (CARTO)\",\n  \"description\": \"Interactive map showing marine biodiversity data from CARTO for Angola coastal waters\",\n  \"table_name\": \"marine_biodiversity\",\n  \"query\": \"SELECT * FROM marine_biodiversity WHERE country = 'Angola' AND the_geom IS NOT NULL\",\n  \"viz_type\": \"points\",\n  \"style\": {\n    \"color\": \"#2E8B57\",\n    \"size\": 8,\n    \"opacity\": 0.8,\n    \"stroke\": \"#1F5F3F\",\n    \"strokeWidth\": 1\n  },\n  \"center\": [-12.5, 18.5],\n  \"zoom\": 6\n}"
            },
