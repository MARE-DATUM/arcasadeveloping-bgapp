        },
        {
          "name": "üê† Marine Species Query",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{BGAPP_AUTH_TOKEN}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"SELECT species_name, scientific_name, conservation_status, COUNT(*) as observations, AVG(depth) as avg_depth, AVG(temperature) as avg_temp FROM marine_biodiversity WHERE country = 'Angola' GROUP BY species_name, scientific_name, conservation_status ORDER BY observations DESC\",\n  \"format\": \"json\"\n}"
            },
            "url": {
              "raw": "{{BGAPP_BASE_URL}}/api/integrations/carto/sql",
              "host": ["{{BGAPP_BASE_URL}}"],
              "path": ["api", "integrations", "carto", "sql"]
            }
          }
        },
        {
          "name": "üåä Coastal Analysis Query",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{BGAPP_AUTH_TOKEN}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"SELECT region, COUNT(*) as species_count, AVG(depth) as avg_depth, MIN(depth) as min_depth, MAX(depth) as max_depth, ST_AsGeoJSON(ST_ConvexHull(ST_Collect(the_geom))) as region_boundary FROM marine_biodiversity WHERE country = 'Angola' AND the_geom IS NOT NULL GROUP BY region ORDER BY species_count DESC\",\n  \"format\": \"json\"\n}"
            },
            "url": {
              "raw": "{{BGAPP_BASE_URL}}/api/integrations/carto/sql",
              "host": ["{{BGAPP_BASE_URL}}"],
              "path": ["api", "integrations", "carto", "sql"]
            }
          }
        }
      ]
    },
