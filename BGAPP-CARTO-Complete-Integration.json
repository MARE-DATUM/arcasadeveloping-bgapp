    {
      "name": "üß† ML Integration with CARTO Data",
      "item": [
        {
          "name": "üéØ ML Prediction with CARTO Data",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{BGAPP_AUTH_TOKEN}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"modelId\": \"marine-species-prediction\",\n  \"data_source\": \"carto\",\n  \"carto_query\": \"SELECT AVG(temperature) as avg_temp, AVG(depth) as avg_depth, AVG(salinity) as avg_salinity FROM marine_biodiversity WHERE location LIKE '%angola%' AND temperature IS NOT NULL\",\n  \"features\": {\n    \"temperature\": 25.5,\n    \"depth\": 100,\n    \"salinity\": 35.2,\n    \"latitude\": -12.5,\n    \"longitude\": 18.5,\n    \"season\": \"summer\",\n    \"use_carto_context\": true\n  },\n  \"options\": {\n    \"confidence_threshold\": 0.8,\n    \"return_probabilities\": true,\n    \"include_similar_species\": true\n  }\n}"
            },
            "url": {
              "raw": "{{BGAPP_BASE_URL}}/ml/predict",
              "host": ["{{BGAPP_BASE_URL}}"],
              "path": ["ml", "predict"]
            }
          }
        },
