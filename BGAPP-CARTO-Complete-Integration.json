            "body": {
              "mode": "raw",
              "raw": "{\n  \"query\": \"SELECT * FROM marine_biodiversity WHERE location LIKE '%angola%' LIMIT 50\",\n  \"format\": \"json\"\n}"
            },
            "url": {
              "raw": "{{BGAPP_BASE_URL}}/api/integrations/carto/sql",
              "host": ["{{BGAPP_BASE_URL}}"],
