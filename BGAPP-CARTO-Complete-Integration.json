        {
          "name": "‚ûï Create Conservation Status Map",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{BGAPP_AUTH_TOKEN}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Conservation Status Map (CARTO)\",\n  \"description\": \"Map showing species conservation status from CARTO data\",\n  \"table_name\": \"marine_biodiversity\",\n  \"query\": \"SELECT *, CASE WHEN conservation_status = 'Endangered' THEN '#FF0000' WHEN conservation_status = 'Vulnerable' THEN '#FF8C00' WHEN conservation_status = 'Near Threatened' THEN '#FFD700' ELSE '#00FF00' END as status_color FROM marine_biodiversity WHERE conservation_status IS NOT NULL AND the_geom IS NOT NULL\",\n  \"viz_type\": \"choropleth\",\n  \"style\": {\n    \"colorField\": \"conservation_status\",\n    \"colorScale\": \"viridis\",\n    \"size\": 10,\n    \"opacity\": 0.7\n  },\n  \"center\": [-12.5, 18.5],\n  \"zoom\": 6\n}"
            },
            "url": {
              "raw": "{{BGAPP_BASE_URL}}/api/integrations/carto/maps",
